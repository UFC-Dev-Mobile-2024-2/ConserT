{
  "expo": {
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "name": "ConserT",
    "slug": "consert",
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/ct-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ct-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hyaza.consert"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ct-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ct-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6022395c-98bf-4702-b554-b06ba60ff181"
      }
    },
    "owner": "lucasvdsgn"
  }
}
